{
  "name": "trading-platform-smb",
  "version": "1.0.0",
  "description": "Trading Platform for Small & Medium Businesses - PostgreSQL Database with Prisma ORM",
  "main": "index.js",
  "scripts": {
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:format": "prisma format",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "trading",
    "smb",
    "ipo",
    "stocks",
    "postgresql",
    "prisma"
  ],
  "author": "Trading Platform Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.0",
    "prisma": "^5.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
